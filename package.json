{
  "name": "@hebcal/leyning",
  "version": "3.2.0",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "keywords": [
    "hebcal",
    "torah",
    "sedra",
    "sedrot",
    "leyning",
    "kriyah",
    "triennial"
  ],
  "description": "Javascript Torah Reading API for Parashat HaShavua and holidays",
  "main": "./dist/index.js",
  "browser": "./dist/bundle.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-leyning.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-leyning/issues"
  },
  "typings": "leyning.d.ts",
  "files": [
    "dist",
    "bin",
    "leyning.d.ts"
  ],
  "bin": {
    "leyning-csv": "bin/leyning-csv"
  },
  "dependencies": {
    "@hebcal/core": "^2.4.0"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublish": "npm run build",
    "readme": "npx jsdoc2md dist/index.js",
    "pretest": "npm run build",
    "test": "ava"
  },
  "license": "BSD-2-Clause",
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/env"
        ]
      }
    },
    "inherit": true,
    "verbose": true
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "ava": "^3.12.1",
    "core-js": "^3.6.5",
    "eslint": "^7.8.1",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^3.6.5",
    "jsdoc-to-markdown": "^5.0.3",
    "rollup": "^2.26.10",
    "rollup-plugin-terser": "^7.0.1"
  }
}
