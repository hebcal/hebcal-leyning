    "require": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-leyning.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-leyning/issues"
  },
  "typings": "leyning.d.ts",
  "files": [
    "dist",
    "bin",
    "leyning.d.ts"
  ],
  "bin": {
    "leyning-csv": "bin/leyning-csv"
  },
  "dependencies": {
    "@hebcal/core": "^3.47.1"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublish": "npm run build",
